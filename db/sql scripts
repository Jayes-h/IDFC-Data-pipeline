CREATE TABLE capital (
    Year INT,
    Net_Worth_Cr VARCHAR(20),
    CET1_Capital_Cr VARCHAR(20),
    Tier1_Capital_Cr VARCHAR(20),
    Tier2_Capital_Cr VARCHAR(20),
    Total_Capital_Cr VARCHAR(20),
    RWA_Cr VARCHAR(20),
    CRAR_Percent FLOAT,
    CET1_Ratio_Percent FLOAT,
    Tier2_Ratio_Percent FLOAT,
    Leverage_Ratio_Percent FLOAT,
    Book_Value_Per_Share VARCHAR(20),
    EPS VARCHAR(20)
);



CREATE TABLE deposit_metrics (
    Year INT,
    Customer_Deposits_Cr BIGINT,
    CASA_Deposits_Cr BIGINT,
    CASA_Ratio_Percent FLOAT,
    Deposit_Growth_YoY_Percent FLOAT,
    Retail_Deposit_Share_Percent FLOAT
);


CREATE TABLE financials (
    Year INT,
    Loans_Advances_Cr FLOAT,
    Deposits_Cr FLOAT,
    CASA_Ratio_Percent FLOAT,
    Gross_NPA_Percent FLOAT,
    Net_NPA_Percent FLOAT,
    PAT_Cr FLOAT,
    ROA_Percent FLOAT,
    ROE_Percent FLOAT
);



CREATE TABLE liabilities (
    Year INT,
    Retail_Deposits_Cr FLOAT,
    Legacy_Borrowings_Cr FLOAT,
    Certificate_of_Deposits_Cr FLOAT,
    Corporate_Deposits_Cr FLOAT,
    Government_Banking_Cr FLOAT,
    Fresh_Borrowings_Cr FLOAT,
    Total_Liabilities_Cr FLOAT,
    Retail_Percent FLOAT,
    Legacy_Percent FLOAT,
    CoD_Percent FLOAT,
    Corporate_Percent FLOAT,
    Govt_Percent FLOAT
);



CREATE TABLE loan_distribution (
    year INT,
    segment FLOAT,
    loan_share_percent FLOAT
);


CREATE TABLE npa_metrics (
    segment VARCHAR(50),
    gross_npa_percent FLOAT,
    net_npa_percent FLOAT
);


CREATE TABLE profitability (
    year INT,
    total_income_cr FLOAT,
    net_profit_cr FLOAT,
    operating_profit_ppop_cr FLOAT,
    net_interest_income_cr FLOAT,
    fee_other_income_cr FLOAT,
    net_interest_margin_percent FLOAT,
    roa_percent FLOAT,
    roe_percent FLOAT
);


CREATE TABLE shareprice (
    month VARCHAR(20),
    bse_high FLOAT,
    bse_low FLOAT,
    bse_volume FLOAT,
    nse_high FLOAT,
    nse_low FLOAT,
    nse_volume FLOAT
);
